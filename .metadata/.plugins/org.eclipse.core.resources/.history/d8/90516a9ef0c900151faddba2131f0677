package com.pack.test;

public class ThreadRunner {
    public static void main(String args[]) {
        ThreadExample previous=null;
        ThreadExample current = null;
        Object obj = new String("This is String object");
        for (int i = 0; i <24; i++) {
            previous=current;
            current = new ThreadExample(obj,previous);
            Thread thread = new Thread(current);
            thread.start();
        }
    }
}
