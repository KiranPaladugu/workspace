package com.pack.test;

public class ThreadRunner {
    public static void main(String args[]) {
        Object obj = new String("This is String object");
        for (int i = 0; i < 5; i++) {
            Thread thread = new Thread(new ThreadExample(obj));
            thread.start();
        }
    }
}
